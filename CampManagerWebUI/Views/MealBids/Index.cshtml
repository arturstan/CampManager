@model IEnumerable<CampManagerWebUI.Models.MealBidViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Lista</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Expend)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PeopleCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Expend)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PeopleCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bid)
        </td>
        <td>
            @Html.ActionLink("Szczegóły", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
