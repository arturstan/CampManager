@model IEnumerable<CampManagerWebUI.Models.SepticTankViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Wywóz szamba</h2>

<p>
    @Html.ActionLink("Utwórz nowy", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KindNameDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Date)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.KindNameDescription)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Amount)
    </td>
    <td>
        @Html.ActionLink("Edycja", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Szczegóły", "Details", new { id = item.Id }) |
        @Html.ActionLink("Usuń", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
